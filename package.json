{
  "name": "samyakkumar",
  "description": "Personal Website",
  "version": "1.0.0",
  "cacheDirectories": [
    "client/node_modules",
    "server/node_modules"
  ],
  "scripts": {
    "start": "cd server/ && npm start",
    "install-client": "npm install --prefix client",
    "install-server": "npm install --prefix server",
    "build-client": "cd client/ && npm build",
    "postinstall": "npm run install-client && npm run install-server && npm run build-client",
    "heroku-postbuild": "cd client && npm install --only=dev -f && npm install -f && npm run build"
  },
  "engines": {
    "npm": "6.9.0",
    "node": "v12.3.1"
  },
  "dependencies": {
    "react-p5-wrapper": "^1.2.1"
  }
}
